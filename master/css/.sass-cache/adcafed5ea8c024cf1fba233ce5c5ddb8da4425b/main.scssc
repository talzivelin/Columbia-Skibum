}

#mainContainer { 
    width: 100%;
    min-width: $contentWidth;

    .sectionWrapper { 
        width: 100%;
        background-repeat: no-repeat;
        background-position: top center;

        &.wrapperVideo { background-color: rgba(0,0,0,0.8); }
        &.wrapperHero { background-image: url('../img/backgrounds/hero_bg_star.jpg'); background-color: #000101; }
