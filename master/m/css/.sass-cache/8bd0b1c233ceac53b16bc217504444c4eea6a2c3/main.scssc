/* ==========================================================================
   HTML5 Boilerplate styles - h5bp.com (generated via initializr.com)
   ========================================================================== */

html,
button,
input,
select,
textarea {
    color: #222;
}

body {
    font-size: 1em;
    line-height: 1.4;
}

::-moz-selection {
    background: #b3d4fc;
    text-shadow: none;
}

::selection {
    background: #b3d4fc;
    text-shadow: none;
}

hr {
    display: block;
    height: 1px;
    border: 0;
    border-top: 1px solid #ccc;
    margin: 1em 0;
    padding: 0;
}

img {
    vertical-align: middle;
}

fieldset {
    border: 0;
    margin: 0;
    padding: 0;
}

textarea {
    resize: vertical;
}

.chromeframe {
    margin: 0.2em 0;
    background: #ccc;
    color: #000;
    padding: 0.2em 0;
}


/* ===== Initializr Styles ==================================================
   Author: Jonathan Verrecchia - verekia.com/initializr/responsive-template
   ========================================================================== */

body {
    font: 16px/26px Helvetica, Helvetica Neue, Arial;
}

.wrapper {
    margin: 0%;
	min-width:320px;
	/* max-width:568px; */
}

/* ===================
    ALL: Orange Theme
   =================== */

.header-container {
    border-bottom: 20px solid #e44d26;
}

.footer-container,
.main aside {
    border-top: 20px solid #e44d26;
}

.header-container,
.footer-container,
.main aside {
    background: #f16529;
}

.title {
    color: white;
}

/* ==============
    MOBILE: Menu
   ============== */

nav ul {
    margin: 0;
    padding: 0;
}

nav a {
    display: block;
    margin-bottom: 10px;
    padding: 15px 0;

    text-decoration: none;
    font-weight: bold;

    color: white;
    background: #e44d26;
}

nav a:hover,
nav a:visited {
    color: white;
}

nav a:hover {
    text-decoration: underline;
}

/* ==============
    MOBILE: Main
   ============== */

.main {
    padding: 30px 0;
}

.main article h1 {
    font-size: 2em;
}

.main aside {
    color: white;
    padding: 0px 5% 10px;
}

.footer-container footer {
    color: white;
    padding: 20px 0;
}

/* ===============
    ALL: IE Fixes
   =============== */

.ie7 .title {
    padding-top: 20px;
}

/* ==========================================================================
   Author's custom styles
   ========================================================================== */

$contentWidth: 100%;


body { 
    @media (min-width: 961px) { 
        overflow-x: hidden;
        width: 100%;
    }
    //overflow-x: hidden;
    min-width: $contentWidth;
    font-weight: lighter;
}

img	{
	width:100%;
	height:auto;
}

$midBlue: #2cb3f8;
$lightCyan: #00ffff;
$brightGreen: #5af93b; 

section.hero {
	
	clear:both;
	text-align: center;
	padding: 0;

	#video-callout {
		margin: 0 auto;
	}

	#hero-graphic {
		display: block;
		margin: 0 auto;
		z-index: 100;
		position: relative;
	}

	#hero-play-btn {

		div {
			display: block;
			width: 40px;
			height: 40px;
			background: url(../img/m-video-play-green.png) 0 top no-repeat;
			margin: 0 auto;
			z-index: 200;
			position: relative;
			text-indent: -9999px;
			margin-top: -20px;
		}	
		
		&:link, &:visited { 
			text-decoration: none;
			color: $brightGreen;
			font-size: 10px;
			line-height: 60px;
			text-transform: uppercase;
			font-weight: bold;
			padding-top: 14px;
		}

		&:hover { color: #fff; div { background-position: bottom; } }
	}

	#hero-cta {
		position: relative;
		margin-top: -110px;
		z-index: 200;

		h2 {
			font-size: 22px;
			color: $midBlue;
			text-transform: uppercase;

			sup {
				font-size: 10px;
				top: -1em;
			}
		}

		p{
			font-size: 16px;
			line-height: 26px;
		}
	}

}

.cta-text {
		color: $lightCyan;
		font-weight: bold;
		font-size: 16px;
		text-transform: uppercase;
		text-decoration: none;
		line-height: 20px;
		display: inline-block;
		background: url(../img/drop-arrow.png) left top no-repeat;
		padding-left: 28px;

		&:hover {
			color: #fff;
			background-position: left bottom;
		}
	}

$blue: #0088ce;
$darkBlue: #10459d;
$lightBlue: #66ccff;

header {
	width: 100%;
	left: 0;
	top: 0;
	background: url(../img/m-blue-90.png) repeat;
	font-family: Arial, sans-serif;
	font-size: 11px;
	text-transform: uppercase;
	float:left;
	margin: 0;
    padding: 0;
	#headerWrap {

		width: 100%;
		margin: 0 auto;
		padding:0;

		h1 {
			display: block;
			position: absolute;
			top: 3px;
			left:10px;
			margin: 0;
			z-index: 9999;

				a {
					display: block;
					width: inherit;
					height: inherit;
				}
		}

		h2 {
			display: block;
			float: right;
			margin-right: 20px;
			text-align: right;

			a {
					display: block;
					width: inherit;
					height: inherit;
				}
		}
		
		img	{
			max-width:50%;
			height:auto;
		}

		nav {
		    display: block;
		    float: left;
		    height: 70px;
		    margin: 0;
		    width: auto;
		    margin-left: 36px;

		    li {
		    	display: inline-block;
		    	height: 70px;
		    	line-height: 70px;
		    	float: none;
		    	width: auto;
		    	padding: 0;
		    	margin: 0;
		    	padding-right: 10px;
		    	margin-right: 10px;
		    	border-right: 1px solid $darkBlue;

		    	a {
		    		margin: 0;
		    		padding: 0;
		    		width: inherit;
		    		display: block;
		    		line-height: 70px;
		    		height: 70px;
		    		float: none;
		    		background: transparent;
		    	}

		    	a.current {
		    		text-decoration: none;
		    		color: $darkBlue;
		    		cursor: default;
		    	}
		    }
		}

		#soc-nav {
			height: 67px;
			float: left; 
			padding-left: 7px;
			overflow: hidden;
			width: 108px;
			padding-top: 3px;

			h6 {
				color: $lightBlue;
				font-size: 11px;
				margin: 0;
			}

			ul {

				margin: 0;
				padding: 0;

				li {
					display: inline-block;
					text-indent: -9999px;
					padding-right: 15px;

					a {
						line-height: 70px;
						display: block;
					}

				}			

				#fb-follow {
					width: 29px;
					height: 29px;
					background: url(../img/fb-lg.png) no-repeat;
				}

				#tw-follow {
					width: 35px;
					height: 28px;
					background: url(../img/tw-lg.png) no-repeat;
				}
			}
		}
	}
}

#mainContainer {
	color: #fff;
}

#mainContainer { 


    .sectionWrapper { 
	
        width: 100%;
		padding:0;
        background-repeat: no-repeat;
        background-position: top center;

        &.wrapperHero { 
			background-color: #000101; 
			float:left;
				width:100%;
		}
		&.wrapperRules { padding: 20px 0 15px; width: 100%; }
		&.wrappersubmitVideos { background-color: #0088ce; float:left; padding:20px 0 15px; width:100%}
        &.wrapperOmniHeat { background-color: black; float:left; padding:20px 0 15px; width:100%}
        &.wrapperFooter { background-color: white; float:left; padding:20px 0 15px; width:100% }

        section, footer { 
            width: 96%;
            margin: 0 auto;
        }
    }

}

.rules { 
	padding-top: 70px;
	a.close { 
		color: #000;
		&:hover { 
			text-decoration: none;
		}
	}
	h3 { 
		color: #0088ce;
	}
	p { 
		color: #0088ce !important;
		margin: 0px;
		padding: 0px;
		color: black;
	}
	ol { 
		list-style: decimal;
		color: #0088ce;
	}
}

/* Submit Video	*/

.submitVideos	{


.viewFullSiteLink	{
			background:url(../img/m-submite_video_link_arrow.png) no-repeat top right;
			text-transform:uppercase;
			color:#00ffff;
			text-decoration:none;
			font-weight:bold;
			padding: 0 30px 0 0;
		}
		
	p	{
	
		margin: 15px 0;
	}	
}

/* mobileShare */

.mobileShare	{

	h3	{
	
		font-size:16px;
		padding:0;
		margin:35px 0 10px;	
	}

	ul	{
		float: left;
		list-style: none;
		margin: 0;
		padding: 0;
		
		li	{
			float:left;
			width:48%;
			margin:0 0 10px;
			padding:0;
		}
		
		li.leftcol	{
			padding:0 2.5% 0 0;
		}
	}
}		

/* Omni Heat */

.omniHeat   {
    background:#000;
    color:#FFF;

        div.omniHeatContent {
            //float:left;
        
            img {
                float:left;
                width:97px;
                margin:0 10px 0 0;
            }   
            p   {
                float:left;
                text-align:left;
                padding:0;
                margin:0;
                color:#FFF;
				margin:25px 0 15px;
            }
        
        }
		
		.exploreOmniHeatLink	{
			background:url(../img/m-omni_heat_link_arrow.png) no-repeat top right;
			text-transform:uppercase;
			color:#e31d3b;
			text-decoration:none;
			font-weight:bold;
			padding: 0 30px 0 0;
		}
		
		.omniheatLeft	{
			float:left;
			width:50%;
			padding:0 23% 20px 0 ;
		}
		
		.omniheatRight	{
			float:right;
			width:27%;
			padding:0 0 20px 0 ;
		}
}

/* Footer */

footer  {
    color: #666666;
    font-size: 12px;
    margin: 20px auto;
    padding: 0;
    width: 100%;
	text-align:center;
    
    p   {
        text-align:center;
        padding:0;
        margin:0;
        line-height:16px;
    }
    
    div {
        float: left;
        width: 90%;
		padding: 10px 5%;
		text-align:center;
        
		
			 
          a:link, a:hover, a:visited  {
                    color:#0088ce;
                    font-weight:bold;
                    text-decoration:none;
                    }
		   
		   img.divline	{
							width:24px;
							height:auto;
						}			
        
         
            }
    .logo   {
                padding: 10px 30%;
    			width: 40%;
            }       
}











/* ==========================================================================
   Media Queries
   ========================================================================== */



/* ==========================================================================
   Helper classes
   ========================================================================== */

.ir {
    background-color: transparent;
    border: 0;
    overflow: hidden;
    *text-indent: -9999px;
}

.ir:before {
    content: "";
    display: block;
    width: 0;
    height: 150%;
}

.hidden {
    display: none !important;
    visibility: hidden;
}

.visuallyhidden {
    border: 0;
    clip: rect(0 0 0 0);
    height: 1px;
    margin: -1px;
    overflow: hidden;
    padding: 0;
    position: absolute;
    width: 1px;
}

.visuallyhidden.focusable:active,
.visuallyhidden.focusable:focus {
    clip: auto;
    height: auto;
    margin: 0;
    overflow: visible;
    position: static;
    width: auto;
}

.invisible {
    visibility: hidden;
}

.clearfix:before,
.clearfix:after {
    content: " ";
    display: table;
}

.clearfix:after {
    clear: both;
}

.clearfix {
    *zoom: 1;
}

/* ==========================================================================
   Print styles
   ========================================================================== */

@media print {
    * {
        background: transparent !important;
        color: #000 !important; /* Black prints faster: h5bp.com/s */
        box-shadow: none !important;
        text-shadow: none !important;
    }

    a,
    a:visited {
        text-decoration: underline;
    }

    a[href]:after {
        content: " (" attr(href) ")";
    }

    abbr[title]:after {
        content: " (" attr(title) ")";
    }

    /*
     * Don't show links for images, or javascript/internal links
     */

    .ir a:after,
    a[href^="javascript:"]:after,
    a[href^="#"]:after {
        content: "";
    }

    pre,
    blockquote {
        border: 1px solid #999;
        page-break-inside: avoid;
    }

    thead {
        display: table-header-group; /* h5bp.com/t */
    }

    tr,
    img {
        page-break-inside: avoid;
    }

    img {
        max-width: 100% !important;
    }

    @page {
        margin: 0.5cm;
    }

    p,
    h2,
    h3 {
        orphans: 3;
        widows: 3;
    }

    h2,
    h3 {
        page-break-after: avoid;
    }
